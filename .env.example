# Environment Variables for Production Deployment
# Copy this to .env.production and fill in your values

# App Configuration
VITE_APP_TITLE=PRAESIDIUM
VITE_APP_DESCRIPTION=Quantum-Resistant E-Governance Platform
VITE_APP_URL=https://your-domain.vercel.app

# API Configuration (when you add backend)
# VITE_API_BASE_URL=https://api.your-domain.com
# VITE_API_VERSION=v1

# Security Keys (for future cryptographic features)
# VITE_PUBLIC_KEY=your-public-key
# VITE_ENCRYPTION_ENABLED=true

# Analytics (optional)
# VITE_ANALYTICS_ID=your-analytics-id

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_ANIMATIONS=true
VITE_ENABLE_PWA=false
